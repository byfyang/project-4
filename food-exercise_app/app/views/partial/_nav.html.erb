<nav>
  <div>
    <ul>
      <% if current_user != nil %>
        <li><%= link_to "Profile", user_path(current_user)%>
        <li><%= link_to "Logout", '/sessions', method: :delete %></li>
      <% else %>
        <li><%= link_to "Signup", new_user_path(@user) %></li>
        <li><%= link_to "Login", '/sign_in' %></li>
    </ul>
  </div>
  <% end %>
</nav>